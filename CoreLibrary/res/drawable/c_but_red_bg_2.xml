<?xml version="1.0" encoding="utf-8"?>  
<selector xmlns:android="http://schemas.android.com/apk/res/android">   
  	<item android:state_pressed="true">
  	    <shape>  
		    <gradient android:startColor="@color/but_stroke_bg" 
		        android:endColor="@color/but_stroke_bg" android:angle="-90"/>
		    <corners android:radius="5dip"/>
		 </shape> 
  	</item>
    <item>
		<shape>  
		    <gradient android:startColor="@color/text_white" 
		        android:endColor="@color/text_white" android:angle="-90"/>  
		    <corners android:radius="5dip"/>   
		    <stroke android:width="0.7dip" android:color="@color/app_bg"/>
		 </shape> 
    </item>
</selector>    
